<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.levis.app.levisapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true">
        <activity
            android:name=".interfaz.Splash"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".interfaz.Search"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".interfaz.MainActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".interfaz.Login"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".interfaz.Register"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".interfaz.Home"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".interfaz.Profile"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".interfaz.Settings"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".interfaz.Photo"
            android:theme="@style/AppTheme" />
    </application>

</manifest>